<h3>Organizations</h3>
<ul>
{{#each model as |organization|}}
  <li>{{organization.name}}</li>
  <ul>
    {{#each organization.repositories as |repository|}}
      <li>{{repository.name}}</li> 
      <ul>
      {{#each repository.users as |user|}}
        <li>{{user.name}}</li>
      {{/each}}
      </ul>
    {{/each}}
  </ul>
{{/each}}
</ul>
{{outlet}}
